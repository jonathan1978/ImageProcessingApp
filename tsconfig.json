{
    "compilerOptions": {              
      "target": "es5",                          
      "module": "commonjs",                     
      "lib": ["ES6"],                            
      "allowJs": true,                    
      "outDir": "./dist",                      
      "strict": true,                          
      "noImplicitAny": true,                 
      "esModuleInterop": true,                 
      "skipLibCheck": true,                    
      "forceConsistentCasingInFileNames": true  
    },
    "exclude": ["node_modules","./dist","spec"]
  } 